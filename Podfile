# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SocialLogins' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Google SignIn
  pod 'GoogleSignIn'
  
  #Pods for Facebook SignIn
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  
  #Pods for Twitter SignIn
  pod 'OAuthSwift'
  
end
